        <description>A counter that is incremented each time the MAC layer receives a unicast.</description>
    </attribute>
    <attribute code="0x0103" type="UNSIGNED_32_BIT_INTEGER" side="server" optional="false" writable="false" reportable="false" minimum="0x00000000" maximum="0xFFFFFFFF" default="0x00000000">
